    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/chrome": "^0.1.32",
