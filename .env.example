# Environment Variables Template
# Copy this file to .env and fill in your values

# ============================================
# Hugging Face
# ============================================
HF_TOKEN=your_huggingface_token_here

# ============================================
# Cloudflare Tunnel Tokens
# ============================================
CLOUDFLARE_TUNNEL_TOKEN_CHAT=your_chat_tunnel_token
CLOUDFLARE_TUNNEL_TOKEN_PHI=your_phi_tunnel_token
CLOUDFLARE_TUNNEL_TOKEN_QWEN=your_qwen_tunnel_token
CLOUDFLARE_TUNNEL_TOKEN_LLAMA=your_llama_tunnel_token
CLOUDFLARE_TUNNEL_TOKEN_MISTRAL=your_mistral_tunnel_token
CLOUDFLARE_TUNNEL_TOKEN_GEMMA=your_gemma_tunnel_token

# ============================================
# GitHub Models (Optional)
# ============================================
GH_MODELS_TOKEN=your_github_models_token

# ============================================
# Model API URLs (for chat-interface)
# ============================================
# Option 1: Use BASE_DOMAIN (recommended)
BASE_DOMAIN=your-base-domain.com

# Option 2: Individual URLs (legacy, used if BASE_DOMAIN not set)
QWEN_API_URL=https://qwen.your-domain.com
PHI_API_URL=https://phi.your-domain.com
LLAMA_API_URL=https://llama.your-domain.com
MISTRAL_API_URL=https://mistral.your-domain.com
GEMMA_API_URL=https://gemma.your-domain.com
VIBEVOICE_API_URL=https://vibevoice.your-domain.com

# ============================================
# GitHub Workflow (for auto-restart)
# ============================================
WORKFLOW_PAT=your_github_personal_access_token_with_repo_scope
